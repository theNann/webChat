<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>org.pyn.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>org.pyn.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>shuoshuo</servlet-name>
        <servlet-class>org.pyn.servlet.ShuoshuoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>shuoshuo</servlet-name>
        <url-pattern>/shuoshuo</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>zone</servlet-name>
        <servlet-class>org.pyn.servlet.ZoneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>zone</servlet-name>
        <url-pattern>/zone</url-pattern>
    </servlet-mapping>
</web-app>